{
  "manifest_version": 2,
  "name": "webnote",
  "description": "This extension is for webnote",
  "version": "0.0.1",
  "permissions": [
    "http://blog.sheimi.me/"
  ],
  "content_scripts": [
    {
      "matches": ["http://blog.sheimi.me/"],
      "js": [
        "content_scripts/jquery-1.11.0.min.js",
        "content_scripts/socket.io.js",
        "content_scripts/webnote.js"
      ],
      "css": ["css/webnote.css"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
